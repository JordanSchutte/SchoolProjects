{{> header }}
<div id="contents">
    <div class="box">
        <div>
            <div class="body">
                <h1>Travel</h1>
                <ul id="sites">
                    {{#each trips}}
                    <li>
                        <a href="travel"><img src="{{this.image}}" alt="{{this.name}}"></a>
                        <h2><a href="travel">{{this.name}}</a></h2>
                        <p>{{this.description}}</p>
                    </li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>
{{> footer }}